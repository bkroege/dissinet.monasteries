{
  "name": "monasteries",
  "version": "1.0.0",
  "description": "scrapping external sources for monasteries in Europe and building a database",
  "main": "index.js",
  "repository": "git@github.com:adammertel/monasteries.git",
  "author": "adammertel <mertel.adam@gmail.com>",
  "license": "MIT",
  "scripts": {
    "scrap": "ts-node scrapping/scrap.ts",
    "validate": "ts-node scrapping/validate.ts",
    "stats": "ts-node scrapping/store-stats.ts",
    "clean-build": "rm -f -r -d build",
    "start": "parcel app/index.html --open --out-dir build",
    "build": "yarn run clean-build && parcel build app/index.html --out-dir build --no-cache --public-url ./",
    "deploy": "rsync -avz build/ hde@hde.geogr.muni.cz:/var/www/virthtml/hde/monasteries --delete",
    "build-deploy": "npm run build && npm run deploy"
  },
  "dependencies": {
    "@turf/truncate": "^6.0.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "bulma": "^0.7.2",
    "cheerio": "^1.0.0-rc.2",
    "chroma-js": "^2.0.2",
    "csvtojson": "^2.0.8",
    "d3": "^5.7.0",
    "fs": "^0.0.1-security",
    "leaflet": "^1.4.0",
    "leaflet.markercluster": "^1.4.1",
    "leaflet.markercluster.placementstrategies": "^0.1.7",
    "mobx": "^5.8.0",
    "mobx-react": "^5.4.3",
    "parcel-bundler": "^1.11.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-leaflet": "^2.1.4",
    "request": "^2.88.0",
    "requests": "^0.2.2",
    "sass": "^1.15.2",
    "ts-node": "^7.0.1",
    "tslint-config-standard": "^8.0.1",
    "turf": "^3.0.14",
    "turf-inside": "^3.0.12",
    "typescript": "^3.2.2",
    "ya-csv": "^0.10.1"
  },
  "devDependencies": {
    "async": "^2.6.1"
  }
}
